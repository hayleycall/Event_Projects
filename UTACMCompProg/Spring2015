Archery.java

import java.io.*;
import java.util.*; 
public class Archery {
	public static void main (String[] args ) 
			throws FileNotFoundException {
		Scanner sc = new Scanner (new File("ArcheryInput"));
	    int testCase = sc.nextInt();
		System.out.print(testCase);
		for (int i = 0; i < testCase; i++) {	
			int arrowsShot = sc.nextInt();
			System.out.print(arrowsShot);
			for (int j= 0; j < arrowsShot; j++) {
				String line = sc.nextLine();
				System.out.print(line);
				Scanner lineScan = new Scanner (line);
				int xCoord = lineScan.nextInt();
				int yCoord = lineScan.nextInt();
				lineScan.nextLine();
				//System.out.print(xCoord);
				//lineScan.close();
			}

			//System.out.print(arrowsShot);
		}
	sc.close();
	}
}


